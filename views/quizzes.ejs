<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Quizzes</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Quizzes</h1>
    <a href="/">Home</a>
    <a href="/credits">Credits</a>
    <a href="/quizzes">Quizzes</a>
    <table>
      <% for (var i in quizzes) { %>
      <% var quiz = quizzes[i]; %>
      <tr>
          <td>
            <%= quiz.question %>
              <br />
          </td>
          <td>
            <%= quiz.answer %>
          </td>
      </tr>
      <% } %>
    </table>

  </body>
</html>
